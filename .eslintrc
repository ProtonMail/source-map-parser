{
  "extends": "standard",
  "rules": {
    "indent": ["error", 4],
    "max-len": ["error", 120],
    "semi": ["error", "always"],
    "space-before-function-paren": ["error", {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
    }],
    "no-sequences": "off",
    "no-else-return": ["error", { "allowElseIf": false }],
    "max-params": ["error", 3],
    "complexity": ["error", 5],
    "curly": "error"
  }
}
